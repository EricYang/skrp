<?xml version="1.0" encoding="utf-8"?>
<s:Button xmlns:fx="http://ns.adobe.com/mxml/2009"
		  xmlns:s="library://ns.adobe.com/flex/spark"
		  xmlns:mx="library://ns.adobe.com/flex/mx"
		  height="28" label="{Fun.R.toMain}" buttonMode="true" click="Fun2.showMain()" fontSize="16"
		  icon="@Embed('x2/image/toMain.png')" rollOut="button_rollOut(event)"
		  rollOver="button_rollOver(event)" toolTip="{Fun.R.tipToMain}">
	
	<fx:Declarations>
		<!-- Place non-visual elements (e.g., services, value objects) here -->
	</fx:Declarations>
	
	<fx:Script>
		<![CDATA[
			import x2.*;
			
			private function button_rollOver(evt:MouseEvent):void { 					
				this.errorString = "Back";
			}
			
			private function button_rollOut(evt:MouseEvent):void {
				this.errorString = "";
			}
		]]>
	</fx:Script>
	
</s:Button>

