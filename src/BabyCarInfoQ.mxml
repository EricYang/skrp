<?xml version="1.0" encoding="utf-8"?>
<s:TitleWindow 
	xmlns:fx="http://ns.adobe.com/mxml/2009" 
	xmlns:s="library://ns.adobe.com/flex/spark"  
	xmlns:mx="library://ns.adobe.com/flex/mx" 
	width="440" height="178" xmlns:x2="x2.*" 
	
  	creationComplete="init()"
	preinitialize="{iR=Fun.getLang('BabyCarInfo')}"
 	>
	<fx:Script>
		<![CDATA[
			import x2.*; import mx.collections.ArrayList; import mx.collections.ArrayCollection;	
			
			[Bindable]
			private var iR:Object;
			
			private function init():void{
				qry_1.aoItem = [
					{source:carCode},
					{source:carNumber}
				];					
			}			
		]]>
	</fx:Script>
	<mx:Form x="6" y="10" horizontalScrollPolicy="off" verticalScrollPolicy="off">
		<mx:FormItem label="{iR.carCode} :">
			<s:TextInput id="carCode"/>
		</mx:FormItem>
		<mx:FormItem label="{iR.carNumber} :">
			<s:TextInput id="carNumber"/>
		</mx:FormItem>
	</mx:Form>
	<x2:comQuery x="348" y="9" id="qry_1">
	</x2:comQuery>
</s:TitleWindow>
