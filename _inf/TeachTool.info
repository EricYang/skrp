{
    "list":{
        "select": "SELECT a.*, b.cardNo, b.cardName, CONCAT(c.cName, ' ', c.eName) AS staffName",
        "from":   "FROM TeachTool a INNER JOIN
                    TeachCard b ON a.teachCardSn = b.sn INNER JOIN
                    Staff c ON a.staffSn = c.sn [_where]",
        "order":  "ORDER BY a.cDate",
        "items": [
            {"fid":"teachCardSn",   "table":"a"},
            {"fid":"cardName",      "table":"b",      "op":"like"},
            {"fid":"staffSn",       "table":"a"}
      ]
    },    
    "progRange": 1,
    "aFOwner": "a.creator",
    "aFOwnerType": "L"
}