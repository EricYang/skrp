{
    "list":{
        "select": "SELECT a.*, CONCAT(a.academicYear, ' - ', a.semester) AS acaName, 
                    CONCAT(b.cName, ' ', b.eName) AS staffName",
        "from": "FROM SelfEvaluateCheck a INNER JOIN
                Staff b ON a.staffSn = b.sn
                [_where]",
        "order": "ORDER BY a.cDate",
        "items": [
            {"fid":"academicYear",      "table":"a"},
            {"fid":"semester",          "table":"a"},
            {"fid":"cDate",             "table":"a",    "op":"between"}
      ]
    },    
    "progRange": 1,
    "aFOwner": "a.creator",
    "aFOwnerType": "L"
}

